{
    "name": "erfanshk/laravel-layered-logic",
    "description": "Layered Repositories, Services and Interfaces",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "framework",
        "laravel-services",
        "services",
        "layered",
        "repository",
        "laravel-repository",
        "laravel-interfaces"
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Erfanshk\\LaravelLayeredLogic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Erfanshk\\LaravelLayeredLogic\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Erfan Shokouhi",
            "email": "shokouhi.erfan75@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11",
        "illuminate/console": "^11",
        "illuminate/contracts": "^11",
        "illuminate/http": "^11"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1",
        "orchestra/testbench": "9.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Erfanshk\\LaravelLayeredLogic\\Providers\\LayeredLogicInitServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
